<h2>Search Product</h2>
<form action="/products" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOption.name %>">
    <button type="submit">Search</button>
</form>
<br>
<% products.forEach((product, index) => { %>
    <div> <%= `${index + 1}. ${product.name}` %></div>
<% }) %>